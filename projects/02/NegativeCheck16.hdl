// Checks if a bus is zero
// Output is 1 if bus == 0 and 0 otherwise

CHIP NegativeCheck16 {
    IN  
        x[16];

    OUT 
        ng;

    PARTS:
    Or(a=x[15], b=x[15], out=ng);



    
}